<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.infohub.project.loginMapper">
	<select id="listAll" resultType="com.infohub.project.login.LoginDTO">
		SELECT * FROM infohub.login
	</select>
	
	<select id="getUserById" resultType="com.infohub.project.login.LoginDTO">
		SELECT * FROM infohub.login where login_id = #{login_id}
	</select>
</mapper>